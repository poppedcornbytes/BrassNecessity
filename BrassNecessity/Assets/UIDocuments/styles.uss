    height: 35px;
    justify-content: flex-start;
    translate: 0 1500px;
    position: absolute;
}

.credit-subheader {
    font-size: 18px;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    -unity-text-outline-color: rgb(255, 255, 255);
    height: 30px;
    justify-content: flex-start;
    translate: 0 1500px;
    position: absolute;
}

.credit-name-and-role {
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    -unity-text-outline-color: rgb(255, 255, 255);
    font-size: 14px;
    -unity-font-style: bold;
    height: 25px;
    justify-content: flex-start;
    position: absolute;
    translate: 0 1500px;
}

.scroll-up-animation {
    transition-duration: 7.5s;
    transition-delay: 0s;
    transition-timing-function: linear;
    translate: 0 -100px;
    transition-property: translate;
}

.game-title {
    -unity-text-align: middle-center;
    font-size: 150px;
    -unity-font-style: normal;
    color: rgb(67, 164, 18);
    -unity-font: url('project://database/Assets/AssetPacks/Fonts/robotica_courtney/Robotica.ttf?fileID=12800000&guid=e8e0f944f27e32b41aa1414645f565da&type=3#Robotica');
    -unity-font-definition: url('project://database/Assets/AssetPacks/Fonts/robotica_courtney/Robotica%20SDF.asset?fileID=11400000&guid=0dcb1aa507b418b4983febf7170c4bd7&type=2#Robotica SDF');
    word-spacing: 5px;
    letter-spacing: 10px;
    text-shadow: 3px 3px 0 rgb(96, 160, 214);
}

.thank-you-title {
    -unity-text-align: middle-center;
    font-size: 75px;
    -unity-font-style: bold;
    -unity-text-outline-width: 3px;
    -unity-text-outline-color: rgb(73, 226, 0);
    color: rgb(39, 115, 0);
}

.continue-title {
    -unity-text-align: upper-center;
    -unity-text-outline-color: rgb(255, 255, 255);
    color: rgb(255, 255, 255);
    font-size: 25px;
    -unity-font-style: bold;
    height: 35px;
    justify-content: flex-start;
}

.transparent-element {
    opacity: 0;
}

.fade-in-animation {
    opacity: 1;
    transition-duration: 1s;
}

.fade-out-animation {
    opacity: 0;
    transition-duration: 0.75s;
}

.heroImage {
    width: 200px;
